  "type": "library",
  "require": {
    "php": ">= 7.2",
    "guzzlehttp/guzzle": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Dragonbe\\Hibp\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^7.2",
    "squizlabs/php_codesniffer": "^3.2",
    "infection/infection": "^0.8.2"
  },
  "autoload-dev": {
    "psr-4": {
      "Dragonbe\\Test\\Hibp\\": "tests/"
    }
  },
